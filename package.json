{
  "name": "@concord-consortium/slate-editor",
  "version": "0.1.1",
  "description": "Slate-based rich text editor component for use in CC projects",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm-run-all clean lint:build build:bundle",
    "build:bundle": "rollup -c",
    "build:dev": "npm-run-all clean lint build:bundle",
    "build:storybook": "build-storybook",
    "clean": "rimraf build",
    "link:react:comment": "For development: cf. https://reactjs.org/warnings/invalid-hook-call-warning.html#duplicate-react",
    "link:react": "npm link ../codap/node_modules/react",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \".eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "list:links": "npm ls -g --depth=0 --link=true",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "unlink:react": "npm unlink -g react"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/slate-editor.git"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.4.1",
    "@types/classnames": "^2.2.10",
    "@types/is-hotkey": "^0.1.1",
    "@types/jest": "^26.0.2",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/slate": "^0.47.7",
    "@types/slate-html-serializer": "^0.6.4",
    "@types/slate-plain-serializer": "^0.7.0",
    "@types/slate-react": "^0.22.9",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.3.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "pretty": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "sass-loader": "^8.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "immutable": "^3.8.2",
    "is-hotkey": "^0.1.6",
    "lodash": "^4.17.15",
    "slate": "^0.47.9",
    "slate-html-serializer": "^0.8.13",
    "slate-plain-serializer": "^0.7.13",
    "slate-react": "^0.22.10",
    "style-to-object": "^0.3.0"
  }
}
