{
  "name": "@concord-consortium/slate-editor",
  "version": "0.10.1",
  "description": "Slate-based rich text editor component for use in CC projects",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all clean lint:build build:bundle clean:build",
    "build:bundle": "rollup -c",
    "build:dev": "npm-run-all clean lint build:bundle clean:build",
    "build:storybook": "storybook build",
    "build:tsc": "tsc",
    "clean": "rimraf dist",
    "clean:build": "rimraf dist/assets dist/common dist/create* dist/editor-toolbar dist/hooks dist/modal-dialog dist/plugin* dist/serialization dist/slate*",
    "link:react:comment": "For development: cf. https://reactjs.org/warnings/invalid-hook-call-warning.html#duplicate-react",
    "link:react": "npm link ../codap/node_modules/react",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \".eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "list:links": "npm ls -g --depth=0 --link=true",
    "prepublish": "npm run build",
    "preversion": "npm-run-all lint test build",
    "start": "npm run storybook",
    "start:clean": "npm run storybook -- --no-manager-cache",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "unlink:react": "npm unlink -g react",
    "yalc:publish": "npx yalc publish --push",
    "yalc:unpublish": "npx yalc installations clean @concord-consortium/slate-editor"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/slate-editor.git"
  },
  "peerDependencies": {
    "react": ">=16.14",
    "react-dom": ">=16.14"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@emotion/core": "^11.0.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-storysource": "^8.2.5",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/cli": "^8.2.5",
    "@storybook/react": "^8.2.5",
    "@storybook/react-webpack5": "^8.2.5",
    "@testing-library/jest-dom": "^6.4.7",
    "@testing-library/react": "^12.1.5",
    "@types/html-escaper": "^3.0.2",
    "@types/is-hotkey": "^0.1.10",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/pretty": "^2.0.3",
    "@types/react": "^16.14.60",
    "@types/react-dom": "^16.9.24",
    "@types/valid-url": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pretty": "^2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "classnames": "^2.5.1",
    "eventemitter3": "^5.0.1",
    "html-escaper": "^3.0.3",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "react-select": "^5.8.0",
    "rgb-hex": "^3.0.0",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-hyperscript": "^0.100.0",
    "slate-react": "~0.98.4",
    "style-to-object": "^1.0.6",
    "tslib": "^2.6.3",
    "valid-url": "^1.0.9"
  }
}
