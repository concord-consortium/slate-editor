{
  "name": "@concord-consortium/slate-editor",
  "version": "0.11.0",
  "description": "Slate-based rich text editor component for use in CC projects",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all clean lint:build build:bundle clean:build",
    "build:bundle": "rollup -c",
    "build:dev": "npm-run-all clean lint build:bundle clean:build",
    "build:storybook": "storybook build",
    "build:tsc": "tsc",
    "clean": "rimraf dist",
    "clean:build": "rimraf dist/assets dist/common dist/create* dist/editor-toolbar dist/hooks dist/modal-dialog dist/plugin* dist/serialization dist/slate*",
    "link:react:comment": "For development: cf. https://reactjs.org/warnings/invalid-hook-call-warning.html#duplicate-react",
    "link:react": "npm link ../codap/node_modules/react",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \".eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "list:links": "npm ls -g --depth=0 --link=true",
    "prepublish": "npm run build",
    "preversion": "npm-run-all lint test build",
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "unlink:react": "npm unlink -g react",
    "yalc:publish": "npx yalc publish --push",
    "yalc:unpublish": "npx yalc installations clean @concord-consortium/slate-editor"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/slate-editor.git"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@emotion/core": "^11.0.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/addon-docs": "9.1.8",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/cli": "^9.1.8",
    "@storybook/react-webpack5": "^9.1.8",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.7",
    "@types/html-escaper": "^3.0.4",
    "@types/is-hotkey": "^0.1.10",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/pretty": "^2.0.3",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/valid-url": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "9.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "npm-run-all": "^4.1.5",
    "pretty": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.2",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "~1.78.0",
    "sass-loader": "^16.0.5",
    "storybook": "^9.1.8",
    "ts-jest": "^29.4.4",
    "typescript": "~5.5.4"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "classnames": "^2.5.1",
    "html-escaper": "^3.0.3",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "react-select": "^5.10.2",
    "rgb-hex": "^3.0.0",
    "slate": "^0.118.1",
    "slate-history": "^0.113.1",
    "slate-hyperscript": "^0.100.0",
    "slate-react": "^0.117.4",
    "style-to-object": "^1.0.9",
    "tslib": "^2.8.1",
    "valid-url": "^1.0.9"
  }
}
