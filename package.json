{
  "name": "@concord-consortium/slate-editor",
  "version": "0.8.2",
  "description": "Slate-based rich text editor component for use in CC projects",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all clean lint:build build:bundle clean:build",
    "build:bundle": "rollup -c",
    "build:dev": "npm-run-all clean lint build:bundle clean:build",
    "build:storybook": "build-storybook",
    "build:tsc": "tsc",
    "clean": "rimraf dist",
    "clean:build": "rimraf dist/assets dist/common dist/create* dist/editor-toolbar dist/hooks dist/modal-dialog dist/plugin* dist/serialization dist/slate*",
    "link:react:comment": "For development: cf. https://reactjs.org/warnings/invalid-hook-call-warning.html#duplicate-react",
    "link:react": "npm link ../codap/node_modules/react",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \".eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "list:links": "npm ls -g --depth=0 --link=true",
    "prepublish": "npm run build",
    "preversion": "npm-run-all lint test build",
    "start": "npm run storybook",
    "start:clean": "npm run storybook -- --no-manager-cache",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "unlink:react": "npm unlink -g react",
    "yalc:publish": "npx yalc publish --push",
    "yalc:unpublish": "npx yalc installations clean @concord-consortium/slate-editor"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/slate-editor.git"
  },
  "peerDependencies": {
    "react": ">=16.14",
    "react-dom": ">=16.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@emotion/core": "^11.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/addons": "^6.5.13",
    "@storybook/cli": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/html-escaper": "^3.0.0",
    "@types/is-hotkey": "^0.1.7",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.190",
    "@types/pretty": "^2.0.1",
    "@types/react": "^16.14.34",
    "@types/react-dom": "^16.9.17",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.28.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "npm-run-all": "^4.1.5",
    "pretty": "^2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.1",
    "sass-loader": "^10.2.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "classnames": "^2.3.2",
    "eventemitter3": "^4.0.7",
    "html-escaper": "^3.0.3",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "react-select": "^5.6.1",
    "rgb-hex": "^3.0.0",
    "slate": "^0.86.0",
    "slate-history": "^0.86.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.86.0",
    "style-to-object": "^0.3.0",
    "tslib": "^2.4.1",
    "valid-url": "^1.0.9"
  }
}
